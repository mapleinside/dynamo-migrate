  "devDependencies": {
    "babel-cli": "^6.6.1",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "dependencies": {
    "aws-sdk": "^2.2.41",
    "lodash": "^4.6.1"
